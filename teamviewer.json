{
    "title": "Swap Left Control/Command only in Teamviewer",
    "rules": [
      {
        "description": "Teamviewer Left Command to Control",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "left_command"
            },
            "to": [
              {
                "key_code": "left_control"
              }
            ],
            "conditions": [
              {
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^com.teamviewer.TeamViewer"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "Teamviewer Left Control to Command",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "left_control"
            },
            "to": [
              {
                "key_code": "left_command"
              }
            ],
            "conditions": [
              {
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^com.teamviewer.TeamViewer"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "Teamviewer left_shift to left_shift",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "left_shift"
            },
            "to": [
              {
                "key_code": "left_shift"
              }
            ],
            "conditions": [
              {
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^com.teamviewer.TeamViewer"
                ]
              }
            ]
          }
        ]
      }
    ]
}